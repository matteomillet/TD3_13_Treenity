<UserControl x:Class="Treenity.UCJeu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Treenity"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1920" Loaded="UserControl_Loaded">
    <UserControl.Background>
        <ImageBrush ImageSource="/Ressources/Images/backgroundForest.jpg"/>
    </UserControl.Background>
    <Canvas x:Name="canvasJeu">
        <Image Source="/Ressources/Images/Branche.png" Stretch="Fill" Height="80" Width="364" Canvas.Left="1304" Canvas.Top="721" />
        <Image Source="/Ressources/Images/Tronc.png" Stretch="Fill" Canvas.Left="1668" Canvas.Top="532"/>
        <Button x:Name="jeuToMenu" Content="Retour au menu" Canvas.Left="734" Canvas.Top="39"/>
    </Canvas>
</UserControl>
